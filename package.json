{
  "name": "vite-plugin-vue-docs",
  "version": "0.0.4",
  "description": "auto document generation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run build -- --watch --ignore-watch example",
    "build": "tsup src/index.ts --dts --format cjs,esm --external @vue/compiler-sfc,chalk,../package.json",
    "example:dev": "npm -C example run dev",
    "patch": "yarn build && npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetqy/vite-plugin-vue-docs.git"
  },
  "keywords": [
    "vue",
    "vite",
    "vite-plugin"
  ],
  "author": "meetqy <meetqy@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetqy/vite-plugin-vue-docs/issues"
  },
  "homepage": "https://github.com/meetqy/vite-plugin-vue-docs#readme",
  "devDependencies": {
    "@types/babel__traverse": "^7.14.0",
    "@types/glob": "^7.1.3",
    "@types/humps": "^2.0.0",
    "@types/node": "^15.12.5",
    "@vue/compiler-sfc": "^3.1.2",
    "tsup": "^4.12.0",
    "typescript": "^4.3.4",
    "vite": "^2.3.8",
    "chalk": "^4.1.1"
  },
  "peerDependencies": {
    "vite": "^2.3.8"
  },
  "dependencies": {
    "@babel/traverse": "^7.14.7",
    "@babel/types": "^7.14.5",
    "glob": "^7.1.7",
    "handlebars": "^4.7.7",
    "humps": "^2.0.1"
  }
}
