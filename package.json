{
  "name": "vite-plugin-vue-docs",
  "version": "0.0.9",
  "description": "vite plugin auto document generation for vue",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "yarn tsup --watch --ignore-watch example & yarn watch:template & yarn example:dev",
    "build": "yarn test && yarn tsup && yarn cp:template",
    "tsup": "tsup src/index.ts --dts",
    "patch": "yarn build && npm version patch",
    "watch:template": "nodemon --watch ./src/template -e hbs,css --exec 'yarn cp:template'",
    "cp:template": "copyfiles -f src/template/* dist/template",
    "example:dev": "npm -C packages/example run dev",
    "test:dev": "jest --watch",
    "test": "jest --coverage && jest-coverage-badges --output './badges'",
    "prepare": "husky install",
    "lint": "eslint src/**/*.ts",
    "fix": "prettier --write src/**/*.{ts,css}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetqy/vite-plugin-vue-docs.git"
  },
  "keywords": [
    "vue",
    "vite",
    "vite-plugin"
  ],
  "author": "meetqy <meetqy@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetqy/vite-plugin-vue-docs/issues"
  },
  "homepage": "https://github.com/meetqy/vite-plugin-vue-docs#readme",
  "devDependencies": {
    "@babel/traverse": "^7.14.7",
    "@babel/types": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/babel__traverse": "^7.14.0",
    "@types/cheerio": "^0.22.30",
    "@types/glob": "^7.1.3",
    "@types/humps": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vue/compiler-sfc": "^3.1.2",
    "babel-jest": "^27.0.6",
    "chalk": "^4.1.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-coverage-badges": "^1.1.2",
    "lint-staged": ">=10",
    "nodemon": "^2.0.9",
    "prettier": "^2.3.2",
    "tsup": "^4.12.0",
    "typescript": "^4.3.4",
    "vite": "^2.3.8"
  },
  "peerDependencies": {
    "vite": "^2.3.8"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "comment-parser": "^1.1.5",
    "glob": "^7.1.7",
    "handlebars": "^4.7.7",
    "humps": "^2.0.1"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/template.ts",
      "src/utils.ts"
    ]
  },
  "lint-staged": {
    "test/**/*.js": [
      "yarn test"
    ],
    "src/**/*.ts": [
      "yarn lint",
      "yarn test"
    ],
    "src/**/*.{ts,css}": "yarn fix"
  },
  "private": true,
  "workspaces": [
    "example"
  ],
  "publishConfig": {
    "access": "public"
  }
}